{
	"name": "@condorhero/vite-plugin-faker",
	"version": "0.0.0",
	"type": "module",
	"license": "MIT",
	"author": "Condor Hero",
	"description": "A fake plugin for Vite use faker.",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.mjs"
		}
	},
	"main": "dist/index.cjs",
	"module": "dist/index.mjs",
	"types": "dist/index.d.cts",
	"typesVersions": {
		"*": {
			"*": [
				"./dist/*",
				"./dist/index.d.ts"
			]
		}
	},
	"files": [
		"dist"
	],
	"keywords": [
		"vite-plugin-faker",
		"vite-plugin-mock",
		"faker-schema-server",
		"faker-server"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/condorheroblog/vite-plugin-faker.git",
		"directory": "packages/vite"
	},
	"bugs": {
		"url": "https://github.com/condorheroblog/vite-plugin-faker/issues"
	},
	"homepage": "https://github.com/condorheroblog/vite-plugin-faker/blob/main/README.md",
	"scripts": {
		"prepublishOnly": "nr build",
		"test": "vitest",
		"dev": "esno ./src/index.ts",
		"build": "rimraf dist && rollup -c",
		"build:watch": "rimraf dist && rollup -c -w",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"faker-schema-server": "workspace:^",
		"path-to-regexp": "^6.2.1"
	},
	"devDependencies": {
		"vite": "^4.4.9"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org"
	}
}
